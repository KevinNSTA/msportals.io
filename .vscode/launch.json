    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "cwd": "${file}",
            "skipFiles": "C:\\Users\\administrator2\\Documents\\PowerShell\\Modules\\Pester\\5.1.1\\Pester.psm1"
        },
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Pester adapter",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/out"
            ]
        }
    ]
}